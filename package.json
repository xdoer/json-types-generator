{
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "a tool which can generate ts types when using restful api",
  "keywords": [
    "restful-api",
    "typescript",
    "types"
  ],
  "repository": "https://github.com/xdoer/restful-api-ts-types-generate",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write \"src/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --parser=typescript --write"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "name": "restful-api-ts-types-generate",
  "author": "xdoer",
  "module": "dist/restful-api-ts-types-generate.esm.js",
  "size-limit": [
    {
      "path": "dist/restful-api-ts-types-generate.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/restful-api-ts-types-generate.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@prequest/node": "^1.0.4",
    "ts-morph": "^11.0.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "lint-staged": "^10.5.3"
  }
}
